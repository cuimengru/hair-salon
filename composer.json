{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "codingyu/ueditor": "^3.0",
        "doctrine/dbal": "2.12",
        "encore/laravel-admin": "1.8.11",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "ghost/google-authenticator": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "ichynul/configx": "^1.4",
        "imnpc/flysystem-aliyun-oss": "^1.3",
        "kra8/laravel-snowflake": "^2.0",
        "laminas/laminas-diactoros": "^2.6",
        "laravel-admin-ext/config": "^1.1",
        "laravel/framework": "^8.45",
        "laravel/horizon": "^5.7",
        "laravel/passport": "^10.1",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.2",
        "leonis/easysms-notification-channel": "^1.0",
        "lustre/php-dfa-sensitive": "^1.4",
        "maatwebsite/excel": "^3.1",
        "mews/captcha": "^3.2",
        "overtrue/laravel-lang": "^5.0",
        "predis/predis": "~1.1",
        "propaganistas/laravel-phone": "^4.3",
        "spatie/laravel-query-builder": "^3.3",
        "vimeo/laravel": "^5.6"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
